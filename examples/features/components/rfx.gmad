! default material will be copper
! default phaseOffset is 0
d1: drift, l=0.5;
rf1: rfx, l=10*cm, E=0.5*MV, frequency=90*MHz, phase=0.02;
rf2: rfx, l=10*cm, gradient=-12*MV / m, frequency=450*MHz;
rf3: rfx, l=10*cm, E=0.1*MV, frequency=90*MHz, tOffset=3.2*ns;
rf4: rfx, l=10*cm, E=0.1*MV, frequency=0*MHz, phase=0.0;
rf5: rfx, l=10*cm, E=0.1*MV, frequency=0*MHz, phase=3.14159;

lat: line=(d1, rf1, d1, rf2, d1, rf3, d1, rf4, d1, d1);
use, lat;

beam, particle="e-", kineticEnergy=1*GeV;