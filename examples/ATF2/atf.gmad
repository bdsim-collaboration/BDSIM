include atf_optics.gmad;
include options.gmad

beam,  particle="e-",
       energy=1.5405110 * GeV,
       nparticles=1e+10,
       distrType="gauss",
       sigmaX=0.002*mm,
       sigmaY=0.002*mm,
       sigmaXp=0.02*0.005,
       sigmaYp=0.02*0.004;


use ,period=ext;

!sample, range = startl;
!sample, range = endl;

option,turnInteractions=1,ngenerate=2000;
option,verboseStep=0;

!print, line;
